<Window x:Class="agregaritemsimpresoraslocal.EditarPieza"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:agregaritemsimpresoraslocal"
        mc:Ignorable="d"
        Title="EditarPieza" Height="150" Width="600" WindowStartupLocation="CenterScreen"  Icon="Untitled-1.ico" ResizeMode="NoResize">
    <Grid>
        <TextBox Name="tbDescripcion" HorizontalAlignment="Left" Height="23" Margin="326,25,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="256"/>
        <TextBox Name="tbCodigo" HorizontalAlignment="Left" Height="23" Margin="191,25,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="130"/>
        <Button Name="btnGuardar" Content="Guardar" HorizontalAlignment="Left" Margin="326,80,0,0" VerticalAlignment="Top" Width="75" Click="BtnGuardar_Click"/>
        <Button Name="btnCancelar" IsCancel="True" Content="Cancelar" HorizontalAlignment="Left" Margin="166,80,0,0" VerticalAlignment="Top" Width="75"/>
        <ComboBox Name="cbINTPieza" HorizontalAlignment="Left" Margin="10,25,0,0" VerticalAlignment="Top" Width="176"/>
    </Grid>
</Window>
